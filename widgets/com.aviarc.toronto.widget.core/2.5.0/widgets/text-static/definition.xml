<widget name="text-static" 
    factory-class="com.aviarc.framework.toronto.widget.DefaultWidgetProviderFactoryImpl"
    container-positioning="y">
    
  <attributes>
    <attribute name="text" mandatory="y" type="string" />
    <attribute name="tooltip" mandatory="n" type="string" />
    <attribute name="action" mandatory="n" type="action" />
  </attributes>
  <includes>
    <js-include file="TextStatic.js"/>
    <css-include file="text-static.css"/>
    <msie6-css-include file="ie6-text-static.css"/>
  </includes>
  <js-constructor>new YAHOO.com.aviarc.framework.toronto.widget.core.v2_5_0.TextStatic()</js-constructor>

</widget>
